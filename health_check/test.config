[size_test_specific]
metric-name = mysqlstat.square_development.holds.SizeBytes
check = >
crit-threshold = 1000000000000
warn-threshold = 1000000000000

[size_test2]
metric-type = developmet.SizeBytes
check = >
crit-threshold = 100000
warn-threshold = 50000
crit-message = size way too big
warn-message = size kinda too big
ok-message = size is ok 

[non-existing metric name]
metric-name = Com_not_a_metric
check = >
crit-threshold = 100000
warn-threshold = 50000
crit-message = size way too big
warn-message = size kinda too big
level-if-not-found = CRIT
message-if-not-found = metric not gathered

